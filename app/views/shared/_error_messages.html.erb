<% if errors.any? %>
  <div class="error_explanation">
    <div class="alert alert-danger">
      <%= t "form_contain" %>
      <%= pluralize errors.count, t("error") %>.
    </div>
    <ul>
      <% errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
    </ul>
  </div>
<% end %>
